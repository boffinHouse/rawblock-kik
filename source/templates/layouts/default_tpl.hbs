<!DOCTYPE html>
 <html class="no-js" lang="de">
    <head itemscope itemtype="http://schema.org/WebSite">
        {{> meta_data }}
        {{> app_icons }}
        {{> head_assets }}

        {{#if component}}
            <link rel="stylesheet" href="docs/css/styleguide.css">
        {{/if}}
    </head>
    <body class="rb-page" {{#if schemaPage}}itemscope itemtype="https://schema.org/{{schemaPage}}"{{/if}}>
        <script> /* block CSS */ </script>
        <div class="page-wrapper">

            <header class="page-header" role="banner">
                <div class="page-header-inner u-column-group u-gutters-horizontal">
                    <div class="u-size-auto">
                        <h1><a href="index.html">RawBlock</a></h1>
                    </div>
                </div>
            </header>

            <main class="page-main" id="main-content" role="main" tabindex="-1">
               {% body %}
            </main>

        </div>

        {{> body_assets}}
    </body>

</html>
